CategoryVariableHere:Button("Airdrop esp",function()
while wait() do
local loc = game:GetService('Players').LocalPlayer.Character.UpperTorso
        local drops = game.Workspace.Drop:GetChildren()
 
        for i,v in pairs(drops) do
            if v.Name == 'Briefcase' then
                if v.Transparency == 0 then
                    local beam = Instance.new("Beam", loc)
                    local A0 = Instance.new("Attachment", loc)
                    local A1 = Instance.new("Attachment", v)
                    beam.Attachment0 = A0
                    beam.Attachment1 = A1
                    beam.Color = ColorSequence.new(Color3.fromRGB(170,0,255),Color3.fromRGB(170,0,255))
                    beam.Width0 = .15
                    beam.Width1 = .15
                    beam.FaceCamera = true
 
                    local espbox = Instance.new("BoxHandleAdornment")
                    espbox.Parent = v
                    espbox.Adornee = v
                    espbox.Size = v.Size
                    espbox.Transparency = 0.5
                    espbox.AlwaysOnTop = true
                    espbox.Color3 = Color3.fromRGB(170,0,255)
                    espbox.ZIndex = 5
                end
            end            
        end end 
end)
